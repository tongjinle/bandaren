.gallery-item {
}

.info {
  display: flex;
  padding: 20rpx 50rpx;
  border-bottom: 1rpx solid #eee;
}

.info .logo {
  width: 120rpx;
  height: 120rpx;
}

.info .center {
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-left: 50rpx;
  padding-top: 15rpx;
}

.info .center .title {
  font-size: 40rpx;
  text-align: left;
}
.info .center .count {
  font-size: 26rpx;
  /* text-align: right; */
  flex: 1;
  display: flex;
  align-items: flex-end;
  color: #666;
}

.info .right {
  display: flex;
  align-items: center;
}

.info .right .more {
  width: 50rpx;
  height: 50rpx;
}
